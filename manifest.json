{
  "manifest_version": 2,

  "name": "Text Speech Plugin",
  "description": "This extension will analyze a page using GTmetrix",
  "version": "1.0",

  
  "background":{
	"scripts":["eventPage.js"],
	"persistent":false
  },
  
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
   ]
}